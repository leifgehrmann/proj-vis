    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vue-tsc": "^1.0.9"
