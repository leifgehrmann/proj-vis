    "@types/node": "^20.5.9",
    "@types/proj4": "^2.5.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
