    "@vitejs/plugin-vue": "^5.1.1",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vue-tsc": "^2.0.29"
